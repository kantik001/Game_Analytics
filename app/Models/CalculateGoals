foreach ($goalsData as $goal) {
        $match = Match::find($goal['match_id']);
        $player = Player::find($goal['player_id']);

        if ($match && $player) {
            Goal::create([
                'match_id' => $goal['match_id'],
                'player_id' => $goal['player_id'],
                'goals' => $goal['goals'],
            ]);
        }
    }
